{
  "name": "yoe-custom-pagination",
  "version": "0.0.1",
  "description": "A customizable pagination component for React",
  "license": "MIT",
  "keywords": [
    "react",
    "react-pagination"
  ],
  "type": "module",
  "main": "./dist/custom-pagination.umd.js",
  "module": "./dist/custom-pagination.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/custom-pagination.es.js",
      "require": "./dist/custom-pagination.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yoe88/react-custom-pagination"
  },
  "author": {
    "name": "yoe88"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "add:lib": "npm install -D ./yoe-custom-pagination-0.0.1.tgz",
    "rm:lib": "npm uninstall @yoe-custom-pagination"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.6",
    "@tailwindcss/vite": "^4.0.6",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@yoe/custom-pagination": "file:yoe-custom-pagination-0.0.1.tgz",
    "path": "^0.12.7",
    "postcss": "^8.5.2",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.6",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0"
  }
}
